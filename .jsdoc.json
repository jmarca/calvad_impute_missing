{
    "source": {
        "include": [ "./trigger_vds_impute.js","./trigger_vds_plot.js","./trigger_wim_impute.js","./trigger_wim_plot.js","./lib"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "encoding": "utf8",               // same as -e utf8
        "destination": "./doc/",          // same as -d ./out/
        "recurse": true,                  // same as -r
        "template":"./node_modules/ink-docstrap/template",
        "package":"package.json",
        "readme": "README.md" // same as -u path/to/tutorials
    }
}
